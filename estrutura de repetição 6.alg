algoritmo "semnome"
// Função :
// Autor :
// Data : 23/04/2017
// Seção de Declarações 
var
ano,c,j:real
inicio
// Seção de Comandos
c <- 1,50
j <- 1,10
ano <- 0
enquanto (j <= c ) faca
 c <- c + 0.02
  j <- j + 0.03
   ano <- ano + 1
fimenquanto
escreva(" são necessarios ", ano, " anos ")

fimalgoritmo
